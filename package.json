{
  "name": "linq-arrays",
  "version": "3.0.0",
  "description": "Emulate LINQ syntax to work with Arrays in JS",
  "main": "./build/index.js",
  "scripts": {
    "format": "prettier --write './{src,tests}/**/*.ts'",
    "rm": "rm -rf build",
    "rm:config": "rm -rf build",
    "build": "tsc -p tsconfig.build.json && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "npm-run-all -s rm build rm:config",
    "test": "vitest run"
  },
  "author": "Allan Garcez <allan.f.garcez@gmail.com> (https://github.com/g4rcez)",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "tslib": "2.5.0",
    "typescript": "5.0.2",
    "vitest": "0.32.0"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
